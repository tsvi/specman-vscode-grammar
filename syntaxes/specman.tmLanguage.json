        },
        {
          "match": "\\b((?:\\d+)?(')(?:[dD])\\s*[0-9_]+)\\b",
          "captures": {
            "1": {"name": "constant.numeric.sized.integer.specman"},
            "2": {"name": "punctuation.separator.literal.specman"}
          }
        },
        {
          "match": "\\b((?:\\d+)?(')(?:[bB])\\s*[0-1_]+)\\b",
          "captures": {
            "1": {"name": "constant.numeric.sized.binary.specman"},
            "2": {"name": "punctuation.separator.literal.specman"}
          }
        },
        {
